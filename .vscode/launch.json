    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch API server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "args": ["service", "start", "--pipeline-dir", "./internal/es/test/pipelines"]
        },
        {
            "name": "Launch API server (mod)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "args": ["service", "start", "--pipeline-dir", "./internal/es/test/test_suite_mod"]
        },
        {
            "name": "cmd pipeline list",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "args": ["pipeline", "list"]
        },
        {
            "name": "cmd pipeline run",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "args": ["pipeline", "run", "github_issue_number", "--pipeline-arg", "github_token='asdf'", "--pipeline-arg", "github_issue_number=2721"]
        },
        {
            "name": "cmd mod init",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "args": ["--mod-location", "/home/vscode/fp-workspace", "mod", "init"]
        },
        {
            "name": "cmd mod install",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            // "args": ["--install-dir", "/home/vscode/.flowpipe", "--mod-location", "/home/vscode/fp-workspace", "mod", "install", "github.com/turbot/steampipe-mod-aws-insights"]
            "args": ["--mod-location", "/home/vscode/fp-workspace", "mod", "install", "github.com/turbot/flowpipe-mod-reference@v1.0.0-beta.1", "--git-url-mode", "ssh"]
        }
    ]
}